<html>

<head>
    <title>Admins Login</title>
    <link rel="stylesheet" href="~/css/StyleSheet1.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="https://fonts.googleapis.com/css?family=Ubuntu" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="stylesheet" href="~/css/fontawesome.min.css">
    
    <title>Sign in</title>
</head>

<body>
    <div class="main">
        <p class="sign" align="center">Sign in</p>
        @{
            if (ViewBag.msg != null)
            {
                <bold><p style="color:red;" align="center">@ViewBag.msg</p></bold>
            }
        }
        <form class="form1" method="post" action="/home/adminM">
            <input name="un" class="un " type="text" align="center" placeholder="Username">
            <input name="pass" class="pass" type="password" align="center" placeholder="Password">
            <br />
            <input class="bt" type="submit" value="Sign In" />
            <br />
            <br />
            <a style="margin-left:40%;" href="/home/index">Homepage</a>
        </form>

    </div>
   
</body>

</html>